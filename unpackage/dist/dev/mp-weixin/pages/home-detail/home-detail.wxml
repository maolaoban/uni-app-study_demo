<view class="detail"><view class="detail-title">{{''+formData.title+''}}</view><view class="detail-header"><view class="detail-avatar"><image src="{{formData.author.avatar}}" mode="aspectFill"></image></view><view class="detail-header_content"><view class="detail-header_name">{{''+formData.author.author_name+''}}</view><view class="detail-header_info"><text>{{formData.create_time}}</text><text>{{formData.browse_count+" 浏览"}}</text><text>{{formData.thumbs_up_count+" 赞"}}</text></view></view><button class="detail-header_button" type="default" data-event-opts="{{[['tap',[['follow',['$0'],['formData.author.id']]]]]}}" bindtap="__e">{{formData.is_author_like?'已关注':'关注'}}</button></view><view class="detail-content"><view class="detail-html"><u-parse vue-id="78d977c0-1" content="{{formData.content}}" noData="{{noData}}" bind:__l="__l"></u-parse></view><view class="detail-comment"><view class="comment-title">最新评论</view><view class="comment-content"><block wx:for="{{commentList}}" wx:for-item="item" wx:for-index="index" wx:key="comment_id"><view><comment-box vue-id="{{'78d977c0-2-'+index}}" comment="{{item}}" data-event-opts="{{[['^reply',[['reply']]]]}}" bind:reply="__e" bind:__l="__l"></comment-box></view></block></view></view></view><view class="detail-bottom"><view data-event-opts="{{[['tap',[['openConment',['$event']]]]]}}" class="detail-bottom_input" bindtap="__e"><text>谈谈你的看法</text><uni-icons vue-id="78d977c0-3" type="compose" size="16" color="#f07373" bind:__l="__l"></uni-icons></view><view class="detail-bottom_box"><view data-event-opts="{{[['tap',[['openComment',['$event']]]]]}}" class="detail-bottom_icons" bindtap="__e"><uni-icons vue-id="78d977c0-4" type="chat" size="22" color="#f07373" bind:__l="__l"></uni-icons></view><view data-event-opts="{{[['tap',[['likeBtn',['$0'],['formData._id']]]]]}}" class="detail-bottom_icons" bindtap="__e"><uni-icons vue-id="78d977c0-5" type="{{formData.is_like?'heart-filled':'heart'}}" size="22" color="#f07373" bind:__l="__l"></uni-icons></view><view data-event-opts="{{[['tap',[['addThumbs',['$0'],['formData._id']]]]]}}" class="detail-bottom_icons" bindtap="__e"><uni-icons vue-id="78d977c0-6" type="{{formData.is_thumbs_up?'hand-thumbsup-filled':'hand-thumbsup'}}" size="22" color="#f07373" bind:__l="__l"></uni-icons></view></view></view><uni-popup class="vue-ref" vue-id="78d977c0-7" type="bottom" maskClick="{{false}}" data-ref="popup" bind:__l="__l" vue-slots="{{['default']}}"><view class="popup-wrap"><view class="popup-header"><text data-event-opts="{{[['tap',[['close',['$event']]]]]}}" class="popup-header_item" bindtap="__e">取消</text><text data-event-opts="{{[['tap',[['submit',['$event']]]]]}}" class="popup-header_item" bindtap="__e">发布</text></view><view class="popup-content"><textarea class="popup-textarea" maxlength="200" fixed="true" placeholder="谈谈你的看法..." data-event-opts="{{[['input',[['__set_model',['','commentValue','$event',[]]]]]]}}" value="{{commentValue}}" bindinput="__e"></textarea><view class="popup-count">{{''+commentValue.length+'/200'}}</view></view></view></uni-popup></view>